{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "export NODE_OPTIONS=â€“max-old-space-size=8192 && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:prod": "node ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d ./src/database/data-source.ts",
    "migration:run:prod": "npm run typeorm:prod migration:run -- -d ./dist/src/database/data-source.js",
    "migration:generate": "npm run typeorm -- -d ./src/database/data-source.ts migration:generate src/migrations/$npm_config_name",
    "migration:generate:prod": "npm run typeorm:prod -- -d ./dist/src/database/data-source.js migration:generate dist/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- -d ./src/database/data-source.ts migration:revert",
    "migration:revert:prod": "npm run typeorm:prod -- -d ./dist/src/database/data-source.js migration:revert",
    "migration:show": "npm run typeorm -- -d ./src/database/data-source.ts migration:show",
    "migration:show:prod": "npm run typeorm:prod -- -d ./dist/src/database/data-source.js migration:show",
    "seed": "ts-node src/seed.ts",
    "seed:prod": "node dist/src/seed.js"
    },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/platform-socket.io": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.5",
    "@types/socket.io": "^3.0.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.1",
    "ioredis": "^5.7.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25"
  }
}
